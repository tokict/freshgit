{
  "name": "freshgit",
  "version": "0.0.1",
  "description": "Your snappy migration helper",
  "author": "Tino Tokic",
  "bin": {
    "oex": "./bin/run"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.3.0",
    "@oclif/plugin-autocomplete": "^1.2.0",
    "@oclif/plugin-commands": "^2.1.0",
    "@oclif/plugin-help": "^5.1.11",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-plugins": "^2.0.1",
    "@types/inquirer": "^8.2.0",
    "axios": "^0.25.0",
    "dotenv": "^16.0.0",
    "inquirer": "^8.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "prettier": "^2.5.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "oex",
    "dirname": "oex",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "migrate": {
        "description": "Migrate user from Github to Freshdesk"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "start": "yarn build && ./bin/run migrate",
    "dev": "./bin/dev migrate",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "GITHUB_TOKEN=fff  FRESHDESK_TOKEN=eeeee mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
